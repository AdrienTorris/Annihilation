<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <ApplicationIcon />
    <StartupObject />
    <Platforms>x64</Platforms>
    <PackageId>Annihilation.JeremieStAmand</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <Authors>Jérémie St-Amand</Authors>
    <Description>.Net Core RTS game with Vulkan graphics</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Prerelease</PackageReleaseNotes>
    <Copyright>Copyright 2017 Jérémie St-Amand. All rights reserved.</Copyright>
    <PackageTags>game engine vulkan</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RootNamespace></RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <PlatformTarget>x64</PlatformTarget>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <BaseIntermediateOutputPath>..\Temp\</BaseIntermediateOutputPath>
    <BaseOutputPath>..\Build\</BaseOutputPath>
    <DocumentationFile></DocumentationFile>
    <DefineConstants>TRACE;DEBUG;NETCOREAPP2_0;WINDOW_WIN32;ENABLE_VALIDATION</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <PlatformTarget>x64</PlatformTarget>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <BaseIntermediateOutputPath>..\Temp\</BaseIntermediateOutputPath>
    <BaseOutputPath>..\Build\</BaseOutputPath>
    <DefineConstants>RELEASE;NETCOREAPP2_0;WINDOW_WIN32</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="obj\**" />
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="obj\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="obj\**" />
    <None Remove="Resources\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Engine\CoreDDL\Data.cs" />
    <Compile Include="Engine\CoreDDL\DataArrayList.cs" />
    <Compile Include="Engine\CoreDDL\DataDescription.cs" />
    <Compile Include="Engine\CoreDDL\DataList.cs" />
    <Compile Include="Engine\CoreDDL\Identifier.cs" />
    <Compile Include="Engine\CoreDDL\Name.cs" />
    <Compile Include="Engine\CoreDDL\OpenDDL.cs" />
    <Compile Include="Engine\CoreDDL\PrimitiveStructure.cs" />
    <Compile Include="Engine\CoreDDL\Property.cs" />
    <Compile Include="Engine\CoreDDL\Reference.cs" />
    <Compile Include="Engine\CoreDDL\Structure.cs" />
    <Compile Include="Engine\CoreDDL\Symbol.cs" />
    <Compile Include="Engine\CoreDDL\Text.cs" />
    <Compile Include="Engine\CoreGEX\File.cs" />
    <Compile Include="Engine\Core\Map.cs" />
    <Compile Include="Engine\Core\String.cs" />
    <Compile Include="Engine\Core\Tree.cs" />
    <Compile Include="Engine\System\Kernel32.cs" />
    <Compile Include="Engine\System\Libdl.cs" />
    <Compile Include="Engine\System\LibraryLoader.cs" />
    <Compile Include="Engine\System\NativeLibrary.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Engine\Resources\" />
  </ItemGroup>

</Project>